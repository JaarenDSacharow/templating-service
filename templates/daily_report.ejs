                                                                        <div class="dataCount waste" style="font-size: 2em;"><%= totalClosedPrescriptions %></div>
                                                                     </div>
                                                                  </div>
                                                                  <div class="dataCountAnomalies intermittent" style="border: 2px solid #5AA24D; border-radius: 5px; color: black; float: left; margin-right: 5px; text-align: center; width: 150px;">
                                                                     <div class="dataCountAnomaliesHeader headerIntermittent" style="background-color:#5AA24D; border: 2px solid #5AA24D; color: white; padding: 6px;">Daily Savings</div>
                                                                     <div class="dataCount waste" style="font-size: 2em; color:#5AA24D">$<%= totalClosedCosts %></div>
                                                                  </div>
                                                                  <div style="float:left;"></div>
                                                                  <div style="float:left;">
                                                                     <h3></h3>
                                                                  </div>
                                                               </td>
                                                            </tr>
                                                         </table>
                                             </div>
                                             <table border="0" cellpadding="0" cellspacing="0" class="btn btn-primary">
                                             <tbody>
                                             <tr>
                                             <td>
                                             &nbsp;
                                             </td>
                                             </tr>
                                             <tr>
                                             <td align="left">
                                             <table border="0" cellpadding="0" cellspacing="0">
                                             <tbody>
                                             <tr>
                                             <td> <a href="https://portal2.entic.com" target="_blank">View More on your portal</a> </td>
                                             </tr>
                                             </tbody>
                                             </table>
                                             </td>
                                             </tr>
                                             </tbody>
                                             </table>
                                             </td>
                                             </tr>
                                             </table>
                                          </td>
                                       </tr>
                                       <!-- END MAIN CONTENT AREA -->
                                    </table>
                                    </tr>



                                 </table>
                                    </div>
                                    
                           <%
                              } 
                              %>
                         
