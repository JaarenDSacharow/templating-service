                                                <% if (waterChange == 'increase') { %>
                                                <img src="http://danthologies.com/images/arrowUP.png" height=50px; />
                                                <% 
                                                   } else { %>
                                                <img src="http://danthologies.com/images/arrowDown.png" height=50px; />
                                                <%
                                                   } 
                                                   %>
                                             </div>

                                          <div style="float:left;">
                                                <h3><%= waterDifferencePercent %>%</h3>
                                             </div>
                                       </td>
                                    </tr>
<!--prescription area -->
                                    <tr>
                                          <div style="float:left; position: relative;right: 25px;">
                                                <table>
                                                   <tr>
                                                      <td class="wrapper">
                                                         <table border="0" cellpadding="0" cellspacing="0">
                                                            <tr>
                                                               <td>
                                                                  <div style="float:left;"><img src="http://danthologies.com/images/prescriptionIcon.png"; height=65px;  /></div>
                                                                  <div style="float:left;">
                                                                     <h2 class="consumption">Prescriptions</h2>
                                                                  </div>
                                                               </td>
                                                            </tr>
                                                         </table>
                                                         <table>
                                                            <tr>
                                                               <td>
                                                                  <div style="float:left; margin-left:25px; ">
